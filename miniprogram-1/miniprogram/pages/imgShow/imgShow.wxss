/* pages/imgShow/imgShow.wxss */
.container{
    position: absolute;
    width: 100%;
    height: 100%;
    font-size: 0;
    overflow: scroll;
}
.imgbg{
    position: absolute;
    width: 100%;
    height: 100%;
    background-color: #000;
}
.imgbox{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    background-color: #fff;
    z-index: 1;
}