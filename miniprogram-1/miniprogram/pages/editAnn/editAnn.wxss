/* pages/editAnn/editAnn.wxss */
.main{
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
}
.top{
    width: 100%;
    height: 100%;
}

.bot,.top{
    margin-top: 40rpx;
}
.list-items{
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    background-color: #fff;
}

.list-item{
    padding: 20rpx;
    font-size: 28rpx;
    height: 40rpx;
    line-height: 40rpx;
    display: flex;
    align-items: center;
}
.icon{
    width: 30rpx;
    height: 30rpx;
    padding: 0 16rpx 0 0;
}